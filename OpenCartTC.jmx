                  <stringProp name="cacheKey">true</stringProp>
                  <stringProp name="script">// Extract category path from Regex Extractor
def categoryPath = vars.get(&quot;categoryPath&quot;) ?: &quot;&quot;
def finalPath = vars.get(&quot;finalCategoryPath&quot;) ?: categoryPath // Preserve previous value if already set

//log.info(&quot;📌 Extracted categoryPath from vars: &apos;${categoryPath}&apos;&quot;)

// Log if `finalCategoryPath` already exists (from a previous request)
def previousFinalPath = vars.get(&quot;finalCategoryPath&quot;) ?: &quot;&quot;
if (previousFinalPath) {
    log.warn(&quot;⚠️ `finalCategoryPath` was already set to &apos;${previousFinalPath}&apos; before this execution.&quot;)
}

// 🚨 If categoryPath is &quot;2558&quot;, redirect it to &quot;20&quot;
if (categoryPath == &quot;2558&quot;) {
    //log.warn(&quot;⚠️ Invalid category detected (2558)! Redirecting to category 20.&quot;)
    finalPath = &quot;20&quot;  // Hardcoded redirect to a valid category
}

// 🚨 If categoryPath is &quot;25&quot;, force navigation to subcategory &quot;25_28&quot;
else if (categoryPath == &quot;25&quot; &amp;&amp; !previousFinalPath) { // Change only if not already set
    //log.info(&quot;🔁 Category 25 detected! Redirecting to subcategory 25_28.&quot;)
    finalPath = &quot;25_28&quot;
} else {
    //log.info(&quot;✅ Normal category selected: ${categoryPath}&quot;)
}

// Prevent unexpected overwrites
if (previousFinalPath &amp;&amp; previousFinalPath != finalPath) {
    //log.error(&quot;🚨 Attempt to change `finalCategoryPath` from &apos;${previousFinalPath}&apos; to &apos;${finalPath}&apos; detected! Keeping previous value.&quot;)
    finalPath = previousFinalPath
}

// Store the correct path for the next request
vars.put(&quot;finalCategoryPath&quot;, finalPath)
//log.info(&quot;🚀 Final Navigation Path Set: ${finalPath}&quot;)

// *** Override the request URL dynamically ***
def baseUrl = &quot;http://172.23.176.132/opencart/upload/index.php?route=product/category&amp;path=&quot;
def newUrl = baseUrl + finalPath

vars.put(&quot;redirectUrl&quot;, newUrl)
//log.info(&quot;🔀 Redirecting JMeter to: ${newUrl}&quot;)
</stringProp>
                </JSR223PreProcessor>
