                  <stringProp name="script">// Extract product ID from JMeter variable
def productId = vars.get(&quot;product_id&quot;) ?: &quot;&quot;
def finalProduct = productId

//log.info(&quot;📌 Extracted ProductId: &apos;${productId}&apos;&quot;)

// 🚨 If productId is &quot;30&quot;, redirect it to &quot;57&quot;
if (productId == &quot;30&quot;) {
    //log.warn(&quot;⚠️ Invalid product detected (30)! Redirecting to product 57.&quot;)
    finalProduct = &quot;57&quot;  // Hardcoded redirect to a valid product
} else {
    //log.info(&quot;✅ Normal product selected: ${productId}&quot;)
}

// Store the correct product ID for the next request
vars.put(&quot;finalProductPath&quot;, finalProduct)
//log.info(&quot;🚀 Final Product Path Set: ${finalProduct}&quot;)

// *** Override the request URL dynamically ***
def finalPath = vars.get(&quot;finalCategoryPath&quot;) ?: &quot;&quot;  // Ensure finalPath exists
def baseUrl = &quot;http://172.23.176.132/opencart/upload/index.php?route=product/product&amp;path=${finalPath}&amp;product_id=&quot;
def newUrl = baseUrl + finalProduct

vars.put(&quot;redirectUrl&quot;, newUrl)
//log.info(&quot;🔀 Redirecting JMeter to: ${newUrl}&quot;)</stringProp>
