                <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Retry Logic" enabled="false">
                  <stringProp name="scriptLanguage">groovy</stringProp>
                  <stringProp name="parameters"></stringProp>
                  <stringProp name="filename"></stringProp>
                  <stringProp name="cacheKey">true</stringProp>
                  <stringProp name="script">import org.apache.http.client.methods.*
import org.apache.http.impl.client.CloseableHttpClient
import org.apache.http.impl.client.HttpClients
import org.apache.http.util.EntityUtils

def maxRetries = 5 // Maximum retry attempts
def attempt = 0

def baseUrl = &quot;http://172.23.176.132/opencart/upload/index.php?route=product/category&amp;path=&quot;
def redirectMap = [&quot;2558&quot;: &quot;20&quot;] // Redirect map for invalid categories

def validCategory = false

CloseableHttpClient httpClient = HttpClients.createDefault()

while (!validCategory &amp;&amp; attempt &lt; maxRetries) {
    attempt++
    log.info(&quot;🔄 Attempt ${attempt} to find a valid category...&quot;)

    // Step 1: Extract categoryPath from JMeter variable
    def categoryPath = vars.get(&quot;categoryPath&quot;) ?: &quot;&quot;

    // Check if the category is in the redirect map
    if (redirectMap.containsKey(categoryPath)) {
        log.warn(&quot;🔀 Category ${categoryPath} is invalid! Redirecting to ${redirectMap[categoryPath]}...&quot;)
        categoryPath = redirectMap[categoryPath]
    }

    if (categoryPath == &quot;25&quot;) {
        log.info(&quot;🔁 Category 25 detected! Redirecting to subcategory 25_28.&quot;)
        categoryPath = &quot;25_28&quot;
    } else {
        log.info(&quot;✅ Valid category selected: ${categoryPath}&quot;)
        validCategory = true
    }

    def categoryUrl = baseUrl + categoryPath
    vars.put(&quot;finalCategoryPath&quot;, categoryPath)

    log.info(&quot;🔍 Sending request to: ${categoryUrl}&quot;)

    // Step 2: Make HTTP Request for Selected Category
    def request = new HttpGet(categoryUrl)
    def response = httpClient.execute(request)
    def responseBody = EntityUtils.toString(response.getEntity())

    // Step 3: Check if Products Exist in the Category
    if (responseBody.contains(&quot;product_id&quot;)) {
        log.info(&quot;✅ Products found in category ${categoryPath}. Proceeding...&quot;)
        validCategory = true
    } else {
        log.warn(&quot;⚠️ No products found in category ${categoryPath}. Retrying...&quot;)
        continue
    }

    log.info(&quot;✅ Successfully selected valid category (${categoryPath})!&quot;)

    break // Exit loop if a valid category is found
}

httpClient.close()

if (!validCategory) {
    log.error(&quot;❌ Max retries reached. Could not find a valid category!&quot;)
    Failure = true
}</stringProp>
                </JSR223PreProcessor>
