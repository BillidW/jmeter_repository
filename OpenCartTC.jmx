                  <stringProp name="script">// Retrieve extracted option IDs (name attributes)
def textInputId = vars.get(&quot;textInput_Id&quot;) ?: &quot;&quot;
def textareaInputId = vars.get(&quot;textareInput_Id&quot;) ?: &quot;&quot;
def radioSelectId = vars.get(&quot;radioSelect_Id&quot;) ?: &quot;&quot;
def dropdownSelectId = vars.get(&quot;dropdownSelect_Id&quot;) ?: &quot;&quot;
def timeSelectId = vars.get(&quot;timeSelect_Id&quot;) ?: &quot;&quot;

// Retrieve extracted option values
def textInputValue = vars.get(&quot;textInput&quot;) ?: &quot;&quot;
def textareaInputValue = vars.get(&quot;textareaInput&quot;) ?: &quot;Default textarea content&quot;
def radioSelectValue = vars.get(&quot;radioSelect&quot;) ?: &quot;&quot;
def dropdownSelectValue = vars.get(&quot;dropdownSelect&quot;) ?: &quot;&quot;
def timeSelectValue = vars.get(&quot;timeSelect&quot;) ?: &quot;&quot;

// Log extracted raw data BEFORE mapping
log.info(&quot;🔍 Extracted IDs -&gt; Text: ${textInputId}, Textarea: ${textareaInputId}, Radio: ${radioSelectId}, Dropdown: ${dropdownSelectId}, Time: ${timeSelectId}&quot;)
log.info(&quot;🔍 Extracted Values -&gt; Text: ${textInputValue}, Textarea: ${textareaInputValue}, Radio: ${radioSelectValue}, Dropdown: ${dropdownSelectValue}, Time: ${timeSelectValue}&quot;)

// Define a map to store option ID → value dynamically
def extractedOptions = [:]

// Validate extracted option IDs before mapping
if (!textInputId) log.error(&quot;⚠️ Text Input ID is missing!&quot;)
if (!textareaInputId) log.error(&quot;⚠️ Textarea ID is missing!&quot;)
if (!radioSelectId) log.error(&quot;⚠️ Radio Select ID is missing!&quot;)
if (!dropdownSelectId) log.error(&quot;⚠️ Dropdown Select ID is missing!&quot;)
if (!timeSelectId) log.error(&quot;⚠️ Time Select ID is missing!&quot;)

// Dynamically assign option IDs to their extracted values (ONLY if both key &amp; value exist)
if (textInputId &amp;&amp; textInputValue) extractedOptions[textInputId] = textInputValue
if (textareaInputId &amp;&amp; textareaInputValue) extractedOptions[textareaInputId] = textareaInputValue
if (radioSelectId &amp;&amp; radioSelectValue) extractedOptions[radioSelectId] = radioSelectValue
if (dropdownSelectId &amp;&amp; dropdownSelectValue) extractedOptions[dropdownSelectId] = dropdownSelectValue
if (timeSelectId &amp;&amp; timeSelectValue) extractedOptions[timeSelectId] = timeSelectValue

// Log extracted final mapping
extractedOptions.each { key, value -&gt;
    sampler.addArgument(key, value)
    log.info(&quot;✅ Added to request: ${key} = ${value}&quot;)
}

// Error handling if extracted options are empty
if (extractedOptions.isEmpty()) {
    log.error(&quot;⚠️ No valid option IDs were extracted! Please check CSS Selectors and Debug Sampler output.&quot;)
}</stringProp>
